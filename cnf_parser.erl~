-module(cnf_parser).
-compile(export_all).

%%Evaldas Karpys cnf._parser
%


read_CNF_File(FileName)->
    Txt = file:read_file(FileName),
    case Txt of
	{ok, Data} -> binary:split(Data, [<<"\n">>], [global]);
		      %print(binary_to_list(Data));
	{error, Reason} -> Reason end.



%print([]) -> ok;
print([L|List]) ->
    L,
    print(List).
    
